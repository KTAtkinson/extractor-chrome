{
  "manifest_version": 2,
  "name": "extractor",
  "version": "0.1",

  "description": "A utility for extracting data from links using user provided perameters.",
  
  "content_scripts": [
    { "matches": ["<all_urls>"],
      "js": [
        "parseUrl.js",
        "extractor_loader.js", 
        "extractor.js"
        ]
    }
  ],
  "permissions": ["clipboardWrite", "contextMenus"]
}


